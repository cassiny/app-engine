  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.13.1"
  },
  "dependencies": {
    "cross-env": "^3.1.4",
    "express": "^4.14.0",
    "winston": "^2.3.0"
