{
  "name": "cassiny-app-engine",
  "version": "0.1.0",
  "description": "Cassiny AppEngine(a.k.a. CAE) is a PaaS platform to build Node.js based web and mobile applications that scale automatically.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev-server": "cross-env NODE_ENV=development nodemon index.js -i src/client/ -i dist/ --exec babel-node",
    "dev-client": "cross-env NODE_ENV=development webpack-dev-server --compress --host 127.0.0.1",
    "build": "babel ./src/server -d ./dist/server && babel ./src/lib -d ./dist/lib",
    "test": "cross-env NODE_ENV=development mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cassiny/app-engine.git"
  },
  "keywords": [
    "cassiny",
    "app-engine",
    "paas",
    "cloud",
    "docker"
  ],
  "author": "cassiny",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/cassiny/app-engine/issues"
  },
  "homepage": "https://github.com/cassiny/app-engine#readme",
